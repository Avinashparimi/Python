SNO=int(input('S.NO of a student is:'))
NAME=input('Name of the student is:')
GENDER=input('Gender of the student is:')
GROUP=input('Group of the student is:')
ADDRESS=input('Address of the student is:')
PHONENUMBER=int(input('Ph.No of the student is:'))
print('DETAILS ARE:')
print('S.No is:',SNO)
print('Name is:',NAME)
print('Gender is:',GENDER)
print('Group is:',GROUP)
print('Address is:',ADDRESS)
print('Ph.No is:',PHONENUMBER)
